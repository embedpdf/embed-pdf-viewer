{
  "name": "@embedpdf/plugin-thumbnail",
  "version": "1.3.1",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preact": {
      "types": "./dist/preact/index.d.ts",
      "import": "./dist/preact/index.js",
      "require": "./dist/preact/index.cjs"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs"
    }
  },
  "scripts": {
    "build:base": "vite build --mode base",
    "build:react": "vite build --mode react",
    "build:preact": "vite build --mode preact",
    "build": "pnpm run clean && concurrently -c auto -n base,react,preact \"vite build --mode base\" \"vite build --mode react\" \"vite build --mode preact\"",
    "clean": "rimraf dist",
    "lint": "eslint src --color",
    "lint:fix": "eslint src --color --fix"
  },
  "dependencies": {
    "@embedpdf/models": "workspace:*"
  },
  "devDependencies": {
    "@embedpdf/build": "workspace:*",
    "@embedpdf/core": "workspace:*",
    "@embedpdf/plugin-render": "workspace:*",
    "@types/react": "^18.2.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@embedpdf/core": "workspace:*",
    "@embedpdf/plugin-render": "workspace:*",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "preact": "^10.26.4"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/embedpdf/embed-pdf-viewer",
    "directory": "packages/plugin-thumbnail"
  },
  "homepage": "https://www.embedpdf.com/docs",
  "bugs": {
    "url": "https://github.com/embedpdf/embed-pdf-viewer/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
